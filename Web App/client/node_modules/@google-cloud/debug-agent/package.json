{
  "name": "@google-cloud/debug-agent",
  "version": "4.2.0",
  "author": "Google Inc.",
  "description": "Stackdriver Debug Agent for Node.js",
  "main": "./build/src/index",
  "types": "./build/src/index.d.ts",
  "repository": "googleapis/cloud-debug-nodejs",
  "keywords": [
    "google",
    "debugger",
    "debug",
    "cloud",
    "cloud debug",
    "cloud debugger",
    "stackdriver debug",
    "stackdriver debugger",
    "stackdriver"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "build/src"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "presamples-test": "npm run compile",
    "samples-test": "cd samples/ && npm link ../ && npm install && npm test && cd ../",
    "presystem-test": "npm run compile",
    "system-test": "mocha build/system-test --timeout 600000",
    "test-no-cover": "cross-env CLOUD_DEBUG_ASSERTIONS=1 mocha build/test --require source-map-support/register --timeout 4000 --R spec",
    "test": "nyc npm run test-no-cover && nyc report",
    "check": "gts check",
    "clean": "gts clean",
    "precompile": "npm run compile-scripts && node build/scripts/setup-build-dir.js",
    "compile": "tsc -p .",
    "compile-scripts": "tsc -p scripts-tsconfig.json",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run check && npm run license-check",
    "prepack": "npm run compile",
    "license-check": "jsgl --local .",
    "lint": "npm run check",
    "docs": "compodoc src/",
    "docs-test": "linkinator docs",
    "predocs-test": "npm run docs"
  },
  "dependencies": {
    "@google-cloud/common": "^2.0.0",
    "@sindresorhus/is": "^1.0.0",
    "acorn": "^7.0.0",
    "coffeescript": "^2.0.0",
    "console-log-level": "^1.4.0",
    "extend": "^3.0.2",
    "findit2": "^2.2.3",
    "gcp-metadata": "^3.0.0",
    "lodash.pickby": "^4.6.0",
    "p-limit": "^2.2.0",
    "semver": "^6.0.0",
    "source-map": "^0.6.1",
    "split": "^1.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@types/acorn": "^4.0.2",
    "@types/console-log-level": "^1.4.0",
    "@types/estree": "0.0.39",
    "@types/extend": "^3.0.0",
    "@types/lodash.pickby": "^4.6.4",
    "@types/mkdirp": "^0.5.1",
    "@types/mocha": "^5.0.0",
    "@types/mv": "^2.1.0",
    "@types/ncp": "^2.0.3",
    "@types/node": "~10.17.0",
    "@types/proxyquire": "^1.3.28",
    "@types/semver": "^6.0.0",
    "@types/tmp": "^0.1.0",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.0.4",
    "cross-env": "^6.0.0",
    "execa": "^3.3.0",
    "gts": "^1.0.0",
    "intelli-espower-loader": "^1.0.1",
    "js-green-licenses": "^1.0.0",
    "linkinator": "^1.5.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "mv": "^2.1.1",
    "ncp": "^2.0.0",
    "nock": "^11.0.0",
    "nyc": "^14.0.0",
    "proxyquire": "^2.0.0",
    "require-inject": "^1.4.3",
    "source-map-support": "^0.5.6",
    "teeny-request": "^5.0.0",
    "tmp-promise": "^2.0.2",
    "typescript": "~3.7.0",
    "uuid": "^3.3.2"
  }

